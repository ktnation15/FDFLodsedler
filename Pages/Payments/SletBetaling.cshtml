@page
@model FDFLodsedler.Pages.Payments.SletBetalingModel
@{
}
<h1>Slet Betaling</h1>
<h3>Er du sikker på du vil slette?</h3>
<div>
    <form method="post">
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.betaling.BørneId)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.betaling.BørneId)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.betaling.PaymentMethod)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.betaling.PaymentMethod)
            </dd>

            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.betaling.Amount)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.betaling.Amount)
            </dd>

        </dl>
        <input type="submit" value="Delete" class="btn btn-danger" />
        <input type="hidden" asp-for="betaling.Betal_Id" />
    </form>
    <div>
        <a asp-page="/Payments/VisBetalings">Tilbage til listen</a>

    </div>
</div>